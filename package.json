{
  "name": "@garrettmk/ts-match",
  "version": "0.0.6",
  "description": "Match objects using declarative JSON.",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean && typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garrettmk/ts-match.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "JSON",
    "match",
    "pattern"
  ],
  "author": "Garrett Myrick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garrettmk/ts-match/issues"
  },
  "homepage": "https://github.com/garrettmk/ts-match#readme",
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.5",
    "typedoc": "^0.22.13",
    "typescript": "^4.5.5"
  }
}
